<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml" xmlns:apn="http://example.org/apn">
  <net id="net-pair" type="http://www.pnml.org/version-2009/grammar/ptnet" netMode="algebraic-int">
    <name><text>Algebraic Pair Operators</text></name>
    <page id="page1">
      <place id="p1">
        <name><text>P1</text></name>
        <graphics><position x="200" y="200"/></graphics>
        <apn:valueTokens>
          <apn:token><apn:text>('key', 5)</apn:text></apn:token>
          <apn:token><apn:text>('key', 8)</apn:text></apn:token>
        </apn:valueTokens>
      </place>
      <place id="p2">
        <name><text>P2</text></name>
        <graphics><position x="600" y="100"/></graphics>
      </place>
      <place id="p3">
        <name><text>P3</text></name>
        <graphics><position x="600" y="300"/></graphics>
      </place>
      <transition id="t1">
        <name><text>T1</text></name>
        <graphics><position x="400" y="200"/></graphics>
        <apn:guard><apn:text>fst(x) == fst(y) and snd(x) != snd(y)</apn:text></apn:guard>
      </transition>
      <arc id="a1" source="p1" target="t1">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>x:Pair, y:Pair</apn:text></apn:binding>
      </arc>
      <arc id="a2" source="t1" target="p2">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>fst(x), snd(x), fst(y), snd(y)</apn:text></apn:binding>
      </arc>
      <arc id="a3" source="t1" target="p3">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>x == y, x != y</apn:text></apn:binding>
      </arc>
    </page>
  </net>
</pnml>

