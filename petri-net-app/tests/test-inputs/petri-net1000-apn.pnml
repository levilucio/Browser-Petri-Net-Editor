      <place id="p1">
        <graphics><position x="40" y="40"/></graphics>
        <apn:type><apn:text>Integer</apn:text></apn:type>
        <initialMarking><text>[1, 2, 3, 4]</text></initialMarking>
      </place>
      <place id="p2">
