<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml" xmlns:apn="http://example.org/apn">
  <net id="net-pair-skeleton" type="http://www.pnml.org/version-2009/grammar/ptnet" netMode="algebraic-int">
    <name><text>Algebraic Pair Skeleton</text></name>
    <page id="page1">
      <place id="p1">
        <name><text>P1</text></name>
        <graphics><position x="200" y="200"/></graphics>
        <apn:valueTokens>
          <apn:token><apn:text>('key', (1, true))</apn:text></apn:token>
          <apn:token><apn:text>('key', (3, false))</apn:text></apn:token>
        </apn:valueTokens>
      </place>
      <place id="p2">
        <name><text>P2</text></name>
        <graphics><position x="600" y="100"/></graphics>
      </place>
      <place id="p3">
        <name><text>P3</text></name>
        <graphics><position x="600" y="300"/></graphics>
      </place>
      <transition id="t1">
        <name><text>T1</text></name>
        <graphics><position x="400" y="200"/></graphics>
        <apn:guard><apn:text>fst(x) == fst(y) and snd(x) != snd(y) and fst(snd(x)) &lt; fst(snd(y)) and snd(snd(x)) != snd(snd(y))</apn:text></apn:guard>
      </transition>
      <arc id="a1" source="p1" target="t1">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>x:Pair, y:Pair</apn:text></apn:binding>
      </arc>
      <arc id="a2" source="t1" target="p2">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>fst(x), snd(x), fst(snd(y)), snd(snd(y)), (fst(x), fst(snd(y)))</apn:text></apn:binding>
      </arc>
      <arc id="a3" source="t1" target="p3">
        <graphics><metadata><sourceDirection>north</sourceDirection><targetDirection>south</targetDirection></metadata></graphics>
        <apn:binding><apn:text>x == y, x != y, fst(x) == fst(y), snd(snd(x)) != snd(snd(y))</apn:text></apn:binding>
      </arc>
    </page>
  </net>
</pnml>


